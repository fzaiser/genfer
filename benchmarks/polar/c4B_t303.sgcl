swapped := 0;
x ~ Geometric(1/2);
y ~ Geometric(1/2);
while (x > 0 and swapped = 0) or (y > 0 and swapped = 1) {
    if 1 ~ Bernoulli(1/3) {
        if swapped = 0 {
            x -= 1;
            swapped := 1;
        } else {
            y -= 1;
            swapped := 0;
        }
    } else if 1 ~ Bernoulli(1/2) {
        if swapped = 0 {
            x -= 2;
            swapped := 1;
        } else {
            y -= 2;
            swapped := 0;
        }
    } else {
        if swapped = 0 {
            x -= 3;
            swapped := 1;
        } else {
            y -= 3;
            swapped := 0;
        }
    }
}
return x;

