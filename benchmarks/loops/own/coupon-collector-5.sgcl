# Coupon collectors with n = 5 coupons

c0 := 0;
c1 := 0;
c2 := 0;
c3 := 0;
c4 := 0;
count := 0;

while c0 = 0 or c1 = 0 or c2 = 0 or c3 = 0 or c4 = 0 {
    c ~ UniformDisc(0, 5);
    if c = 0 {
        c0 := 1;
    } else if c = 1 {
        c1 := 1;
    } else if c = 2 {
        c2 := 1;
    } else if c = 3 {
        c3 := 1;
    } else {
        c4 := 1;
    }
}

return count;
