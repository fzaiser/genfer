# flags(geobound-ev): -u 70 -d 3 --solver adam-barrier --optimizer adam-barrier
# flags(geobound-tail): -d 3 --solver adam-barrier --optimizer adam-barrier

x := 1;
y := 1;
n := 0;
while x != 0 or y != 0 {
    if flip(1/2) {
        if flip(1/4) {
            x += 1;
        } else {
            x -= 1;
        }
    } else {
        if flip(1/4) {
            y += 1;
        } else {
            y -= 1;
        }
    }
    n += 1;
}
return n;
